{
  "name": "mono-ui",
  "version": "1.1.11",
  "description": "A composable, dark themed UI Framework specifically designed to work with TailwindCSS.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf dist && npm run tailwind:css && webpack",
    "tailwind:css": "tailwind build src/tailwind.css -c tailwind.config.js -o src/index.css",
    "prepare": "npm run build"
  },
  "keywords": [],

  "license": "ISC",
  "peerDependencies": {
    "react": "^16.13.1"
  },
  "devDependencies": {
    "source-map-loader": "^0.2.4",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "react-custom-scrollbars": "^4.2.1"
  },
  "dependencies": {
    "tailwindcss": "^1.2.0"
  }
}
